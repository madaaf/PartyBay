<resources>



    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar"></style>

    <style name="ThemeWithCorners" parent="android:Theme.DeviceDefault.Dialog.NoActionBar">
        <item name="android:windowBackground">@drawable/cornertheme</item>
    </style>
    <style name="Item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">300dp</item>
    </style>

    <style name="Item.Top">
        <item name="android:background">@color/white</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="Item.TopTransparent">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="Item.Sticky">
        <item name="android:background">@color/sticky_item</item>
        <item name="android:layout_height">@dimen/sticky_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">?android:textAppearanceLarge</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textColor">@color/sticky_item_text</item>
    </style>

